{
    "name": "sunaoka/aws-sdk-php-structures-generator",
    "description": "",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "8.4.*",
        "aws/aws-sdk-php": "3.342.19",
        "nette/php-generator": "^4.1.7",
        "symfony/console": "^7.2.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.67.0",
        "phpstan/phpstan": "^2.1.1",
        "phpunit/phpunit": "^11.5.2",
        "symfony/var-dumper": "^7.2.0"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\Aws\\Structures\\": "classes/",
            "Sunaoka\\Aws\\Structures\\Generator\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
